<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity OwnedMedicine.
    -->
    <changeSet id="20190914162403-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="owned_medicine"
                                 constraintName="fk_owned_medicine_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="owned_medicine_id"
                                 baseTableName="owned_medicine_medicine"
                                 constraintName="fk_owned_medicine_medicine_owned_medicine_id"
                                 referencedColumnNames="id"
                                 referencedTableName="owned_medicine"/>

        <addForeignKeyConstraint baseColumnNames="medicine_id"
                                 baseTableName="owned_medicine_medicine"
                                 constraintName="fk_owned_medicine_medicine_medicine_id"
                                 referencedColumnNames="id"
                                 referencedTableName="medicine"/>
        
    </changeSet>
</databaseChangeLog>
